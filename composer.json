{
    "name": "moss/storage",
    "version": "v0.10",
    "description": "moss storage",
    "keywords": [
        "orm",
        "storage",
        "data mapper",
        "database"
    ],
    "type": "orm",
    "license": "MIT",
    "authors": [
        {
            "name": "Michal Wachowski",
            "email": "wachowski.michal@gmail.com"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": ">=5.4",
        "doctrine/dbal": ">=2.5"
    },
    "require-dev": {
        "phpunit/phpunit": ">=3.7",
        "phpmd/phpmd": "1.5.*",
        "squizlabs/php_codesniffer": "1.5.*"
    },
    "suggest": {
        "psr/log": "For logging queries via LoggingDecorator"
    },
    "autoload": {
        "psr-0": {
            "Moss\\Storage\\": ""
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Moss\\Storage\\": "./tests/"
        }
    }
}
